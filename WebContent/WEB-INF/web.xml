    <servlet-name>Update_schedule</servlet-name>
    <servlet-class>servlet.UpdateSchedule</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Update_schedule</servlet-name>
    <url-pattern>/UpdateSchedule</url-pattern>
  </servlet-mapping>
  <servlet>
		<display-name>InsertNews</display-name>
		<servlet-name>Insert_News</servlet-name>
		<servlet-class>servlet.InsertNews</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Insert_News</servlet-name>
		<url-pattern>/InsertNEWS</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<display-name>ViewPendingReq</display-name>
		<servlet-name>View_Pending_Req</servlet-name>
		<servlet-class>servlet.ViewPendingReq</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>View_Pending_Req</servlet-name>
		<url-pattern>/ViewPendingReq</url-pattern>
	</servlet-mapping>

	<servlet>
		<display-name>ConfirmReq</display-name>
		<servlet-name>Confirm_Req</servlet-name>
		<servlet-class>servlet.confirmReq</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Confirm_Req</servlet-name>
		<url-pattern>/ConfirmReq</url-pattern>
	</servlet-mapping>
<servlet>
		<display-name>UpdateNews</display-name>
		<servlet-name>Update_News</servlet-name>
		<servlet-class>servlet.UpdateNews</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Update_News</servlet-name>
		<url-pattern>/UpdateNEWS</url-pattern>
	</servlet-mapping>

	<servlet>
		<display-name>DeleteNews</display-name>
		<servlet-name>Delete_News</servlet-name>
		<servlet-class>servlet.DeleteNews</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Delete_News</servlet-name>
		<url-pattern>/DeleteNEWS</url-pattern>
	</servlet-mapping>
  <filter>
    <filter-name>continuation</filter-name>
    <filter-class>org.eclipse.jetty.continuation.ContinuationFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>continuation</filter-name>
    <url-pattern>/cometd/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>cometd</servlet-name>
    <servlet-class>org.cometd.annotation.AnnotationCometdServlet</servlet-class>
    <init-param>
      <param-name>timeout</param-name>
      <param-value>20000</param-value>
    </init-param>
    <init-param>
      <param-name>interval</param-name>
      <param-value>0</param-value>
    </init-param>
    <init-param>
      <param-name>maxInterval</param-name>
      <param-value>10000</param-value>
    </init-param>
    <init-param>
      <param-name>maxLazyTimeout</param-name>
      <param-value>5000</param-value>
    </init-param>
    <init-param>
      <param-name>long-polling.multiSessionInterval</param-name>
      <param-value>2000</param-value>
    </init-param>
    <init-param>
      <param-name>logLevel</param-name>
      <param-value>0</param-value>
    </init-param>
    <init-param>
      <param-name>transports</param-name>
      <param-value>org.cometd.websocket.server.WebSocketTransport</param-value>
    </init-param>
    <init-param>
      <param-name>services</param-name>
      <param-value>com.semika.cometd.ChatService</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>cometd</servlet-name>
    <url-pattern>/cometd/*</url-pattern>
  </servlet-mapping>
</web-app>