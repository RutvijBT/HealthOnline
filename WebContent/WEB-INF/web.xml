    <servlet-name>ViewProfile</servlet-name>
    <servlet-class>servlet.Viewprofile</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewProfile</servlet-name>
    <url-pattern>/ViewProfile</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>ViewPendingReq</display-name>
    <servlet-name>ViewPendingReq</servlet-name>
    <servlet-class>servlet.ViewPendingReq</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewPendingReq</servlet-name>
    <url-pattern>/ViewPendingReq</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>ViewRegDetails</display-name>
    <servlet-name>View_Reg_Details</servlet-name>
    <servlet-class>servlet.ViewRegDetails</servlet-class>
  </servlet>
  <servlet-mapping>
