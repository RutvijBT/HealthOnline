  </servlet>
  <servlet-mapping>
    <servlet-name>PatientSelfDescription</servlet-name>
    <url-pattern>/description.post</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>delete data of patient medical history</display-name>
    <servlet-name>DeleteMedicalHistory</servlet-name>
    <servlet-class>servlet.DeleteMedicalHistory</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteMedicalHistory</servlet-name>
    <url-pattern>/medical.delete</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>Registration</display-name>
    <servlet-name>Registration</servlet-name>
    <servlet-class>servlet.Registration</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Registration</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
